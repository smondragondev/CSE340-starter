<% if (title) { %>
<h1><%= title %></h1>
<% } else {
  res.redirect('/')
} %>

<%- messages() %>
<div class="account-welcome">
  <h2>Welcome <%= locals.accountData.account_firstname %></h2>
  <p><a href="/account/edit/<%= locals.accountData.account_id %>">Update Account Information</a></p>
</div>
<% if (locals.canManage) { %>
<div class="inventory-management">
  <h3>Inventory Management</h3>
  <p> <a href="/inv/">Click Here</a></p>
</div>
<% } %>